{
    "name": "flourishlabs/saloon-openvidu",
    "description": "Saloon SDK for OpenVidu",
    "keywords": [
        "ashleyhindle",
        "flourishlabs",
        "OpenVidu",
        "saloon",
        "saloon-openvidu"
    ],
    "homepage": "https://github.com/flourishlabs/saloon-openvidu",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashley Hindle",
            "email": "hey@ashleyhindle.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "FlourishLabs\\SaloonOpenVidu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlourishLabs\\SaloonOpenVidu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
